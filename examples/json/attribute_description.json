{
	"occi.core.id": {
		"type": "string",
		"pattern": {
			"type": "string",
			"pattern": "[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}"
		}
	},
	"occi.core.title": {
		"type": "string",
		"mutable": true,
		"pattern": {
			"type": "string",
			"pattern": ".*"
		}
	},
	"occi.core.summary": {
		"type": "string",
		"mutable": true,
		"pattern": {
			"type": "string",
			"pattern": ".*"
		}
	},
	"compute.architecture": {
		"type": "string",
		"mutable": true,
		"pattern": {
			"type": "string",
			"pattern": "x86|x64"
		}
	},
	"compute.cores": {
		"type": "number",
		"mutable": true,
		"pattern": {
			"type": "number"
		}
	},
	"compute.hostname": {
		"type": "string",
		"mutable": true,
		"pattern": {
			"type": "string",
			"pattern": "(([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9\\-]*[a-zA-Z0-9])\\.)*"
		}
	},
	"compute.memory": {
		"type": "number",
		"mutable": true,
		"pattern": {
			"type": "number"
		}
	},
	"compute.speed": {
		"type": "number",
		"mutable": true,
		"pattern": {
			"type": "number"
		}
	},
	"compute.state": {
		"type": "string",
		"pattern": {
			"type": "string",
			"pattern": "inactive|active|suspended|error"
		}
	}
}
